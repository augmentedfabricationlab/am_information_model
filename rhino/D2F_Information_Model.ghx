from compas.geometry import Frame
from compas.geometry import Line
from compas.geometry import Vector

from am_information_model.model import Node
from am_information_model.model import Layer

def _radius_between_frames(from_frame, via_frame, to_frame, max_radius):
        in_line = Line(from_frame.point, via_frame.point)
        out_line = Line(via_frame.point, to_frame.point)
        r = min(max_radius, in_line.length/2, out_line.length/2)
        return r

# nodes
print crv
pts = rs.DivideCurveLength(crv, div_length)
max_r = div_length/2
nodes = []
frames = [Frame([pt.X, pt.Y, pt.Z], [0, -1, 0], [-1, 0, 0]) for pt in pts]

for i,frame in enumerate(frames):
    r = _radius_between_frames(frames[max(0,i-1)], frame, frames[min(i+1, len(frames)-1)], max_r)
    nodes.append(Node(frame, radius=r))
layer = Layer.from_nodes(nodes)</item>
