                    <item name="CodeInput" type_name="gh_string" type_code="10">import time
from scriptcontext import sticky as st
from fabrication_control.task import Task
from ur_fabrication_control.direct_control.fabrication_process import URTask
from ur_fabrication_control.direct_control import URScript

ur_ip = st["ur_ip"]
ur_port = st["ur_port"]
server_ip = st["server_ip"]
server_port = st["server_port"]

def create_tasks_from_am_model(am_model):
    tasks = []
    for key, layer in am_model.layers():
        req_msg = "Layer{}_completed".format(key)
        urscript = urscript_from_layer(layer, key, req_msg)
        new_task = URTask(server, urscript, req_msg, key)
        tasks.append(new_task)
    return tasks

def get_frames_in_RCS(frames):
    return [robot.to_local_coordinates(frame) for frame in frames]

def urscript_from_layer(layer, layer_number, exit_message):
    ur_script = URScript(ur_ip=ur_ip, ur_port=ur_port)
    tool_angle_axis = list(robot.attached_tool.frame.point) + list(robot.attached_tool.frame.axis_angle_vector)
    ur_script.start()
    ur_script.set_tcp(tool_angle_axis)
    RCS_frames = get_frames_in_RCS([node.frame for key, node in layer.nodes()])
    radii = [node.radius for key, node in layer.nodes()]
    ur_script.add_line("textmsg(\"&gt;&gt; TASK {}.\")".format(layer_number))
    # Initialize first movement (with accelleration)
    ur_script.socket_open(server_ip, server_port, "Feedbackserver")
    ur_script.move_linear(RCS_frames[0], robot_speed, 0.0)
    node_msg = {"LAYER":layer_number, "NODE":0}
    ur_script.socket_send_line_string(str(node_msg), socket_name="Feedbackserver")
    # Send all next frames of the layer (constant speed, no accelleration)
    for key, RCS_frame in enumerate(RCS_frames[1:len(RCS_frames)]):
        ur_script.move_process(RCS_frame, robot_speed, radii[key])
        node_msg = {"LAYER":layer_number, "NODE":key}
        ur_script.socket_send_line_string(str(node_msg), socket_name="Feedbackserver")
    ur_script.socket_close(name="Feedbackserver")# BS: Closing socket at the end of the loop
    # Signify ready
    ur_script.socket_open(server_ip, server_port, "Feedbackserver")
    ur_script.socket_send_line_string(exit_message, socket_name="Feedbackserver")
    ur_script.socket_close(name="Feedbackserver")# BS: Closing socket at the end of the loop
    #Finalize script
    ur_script.end()
    ur_script.generate()
    return ur_script

if create:
    st["tasks"] = create_tasks_from_am_model(am_model)

tasks = st["tasks"]</item>
