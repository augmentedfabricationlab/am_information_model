frames = [Frame([pt.X, pt.Y, pt.Z], [0, -1, 0], [-1, 0, 0]) for pt in pts]

for i,frame in enumerate(frames):
    r = _radius_between_frames(frames[max(0,i-1)], frame, frames[min(i+1, len(frames)-1)], max_r)
    nodes.append(Node(frame, radius=r))
layer = Layer.from_nodes(nodes)</item>
