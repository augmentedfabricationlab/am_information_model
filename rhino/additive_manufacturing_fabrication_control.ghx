                    <item name="CodeInput" type_name="gh_string" type_code="10">from scriptcontext import sticky as st
from fabrication_control.fabrication import Fabrication

if init:
    fabrication = Fabrication(server)
    fabrication.parallelize = parallelize
    st["fabrication"] = fabrication

fabrication = st["fabrication"]

if set_tasks:
    print "setting tasks"    
    fabrication.clear_tasks()
    fabrication.set_tasks(tasks)

if start:
    #if robot.is_connected(): #and arduino.is_connected():
    print "starting process"
    fabrication.start()

if stop:
    print "stopping process"
    fabrication.stop()
    
if reset:
    print "resetting process"
    fabrication.reset()

server = fabrication.server

