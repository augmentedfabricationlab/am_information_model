print(configuration)

if robot and configuration:
    
    robot.update(configuration, visual=show_visual, collision=show_collision)
    ur_params = ur_params[robot.name]
    
    if show_visual:
        visual = robot.artist.draw_visual()

    if show_collision:
        collision = robot.artist.draw_collision()
    
